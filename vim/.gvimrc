" Powerline font
"set guifont=Source\ Code\ Pro\ for\ Powerline:h13
" set guifont=Inconsolata\ for\ Powerline:h18
set guifont=Monaco\ for\ Powerline:h18

" Solarized stuff
" let g:solarized_contrast="high"    "default value is normal
" let g:solarized_visibility="normal"    "default value is normal
syntax enable
set background=dark
" colorscheme gruvbox
" colorscheme mustang
colorscheme tomorrow-night-eighties

" Hide scrollbars
:set guioptions-=L

" Make spacing smaller to fit powerline on the screen
:set linespace=0
